%p#notice= notice

%p
  Room contains
%p
  = @dungeon_content

%hr

#top-arrow-div
  - if @directions.include?( :top )
    = link_to [ @dungeon_instance, { direction: :top } ], { method: :patch, class: 'direction-arrow' } do
      <img src="data:image/svg+xml;utf8;base64," />

#left-arrow-div
  - if @directions.include?( :left )
    = link_to [ @dungeon_instance, { direction: :left } ], { method: :patch, class: 'direction-arrow'  } do
      <img class='direction-arrow' src="data:image/svg+xml;utf8;base64," />

#current_room_div
  = image_tag( 'current_room.jpg', class: :current_room )

#right-arrow-div
  - if @directions.include?( :right )
    = link_to [ @dungeon_instance, { direction: :right } ], { method: :patch, class: 'direction-arrow'  } do
      <img src="data:image/svg+xml;utf8;base64," />

#bottom-arrow-div
  - if @directions.include?( :bottom )
    = link_to [ @dungeon_instance, { direction: :bottom } ], { method: :patch, class: 'direction-arrow'  } do
      <img src="data:image/svg+xml;utf8;base64," />